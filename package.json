{
  "name": "budgetbuddyapi",
  "version": "1.0.0",
  "description": "API for BudgetBuddy",
  "main": "build/index.js",
  "scripts": {
    "build": "NODE_ENV=production babel src --out-dir build --copy-files --no-copy-ignored",
    "dev": "NODE_ENV=development nodemon --exec babel-node src/index.js",
    "start": "NODE_ENV=production node build/index.js",
    "migrate": "node ./runMigration",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000 --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alaneicker1975/ButdgetBuddyAPI.git"
  },
  "keywords": [
    "Node.js",
    "PostgeSQL",
    "API"
  ],
  "author": "Alan Eicker",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alaneicker1975/ButdgetBuddyAPI/issues"
  },
  "homepage": "https://github.com/alaneicker1975/ButdgetBuddyAPI#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.21.0",
    "helmet": "^6.1.5",
    "joi": "^17.9.2",
    "jsonschema": "^1.4.1",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.10.0",
    "postgres-migrations": "^5.3.0",
    "swagger-ui-express": "^4.6.3",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.21.4",
    "cross-env": "^7.0.3",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
